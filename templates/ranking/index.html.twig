{% extends 'layout.html.twig' %}

{% block title %}Freelancers Ranking
{% endblock %}

{% block body %}
    {% for user in users %}
        <div>
            <img src="{{user.image}}" alt="picture off {{user.username}}">
            <h1>{{user.username}}</h1>
            <p>{{ user.email }}</p>
            <p>lvl:
                {{ user.level }}</p>
            <p>exp:
                {{ user.experience }}</p>
            <a href="#" class="btn btn-primary btn-sm">View my profile</a>
        </div>
    {% endfor %}
{% endblock %}
