{% extends 'layout.html.twig' %}

{% block title %}Help{% endblock %}

{% block body %}
    <h1>Help</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ help.id }}</td>
            </tr>
            <tr>
                <th>Subject</th>
                <td>{{ help.subject }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ help.description }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ help.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ help.createdAt ? help.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ help.updatedAt ? help.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_help_index') }}">back to list</a>

    <a href="{{ path('admin_help_edit', {'id': help.id}) }}">edit</a>

    {{ include('admin_help/_delete_form.html.twig') }}
{% endblock %}
